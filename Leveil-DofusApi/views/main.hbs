            {{#if re_f_neutre}}<li>Résistance fix neutre: {{re_f_neutre}}</li>{{/if}}
            {{#if re_f_terre}}<li>Résistance fix terre: {{re_f_terre}}</li>{{/if}}
            {{#if re_f_feu}}<li>Résistance fix feu: {{re_f_feu}}</li>{{/if}}
            {{#if re_f_chance}}<li>Résistance fix chance: {{re_f_chance}}</li>{{/if}}
            {{#if re_f_air}}<li>Résistance fix air: {{re_f_air}}</li>{{/if}}
            {{#if re_x_neutre}}<li>Résistance % neutre: {{re_x_neutre}}</li>{{/if}}
