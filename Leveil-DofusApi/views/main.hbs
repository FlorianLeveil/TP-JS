            {{#if dmg_x_distance}}<li>Dommage % distance: {{dmg_x_distance}}</li>{{/if}}
            {{#if dmg_x_arme}}<li>Dommage % arme: {{dmg_x_arme}}</li>{{/if}}
            {{#if dmg_x_sorts}}<li>Dommage % sorts: {{dmg_x_sorts}}</li>{{/if}}
            {{#if dmg_crit}}<li>Dommage critique: {{dmg_crit}}</li>{{/if}}
            {{#if dmg_pousse}}<li>Dommage poussée: {{dmg_pousse}}</li>{{/if}}
