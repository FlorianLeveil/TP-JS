            {{#if pui_piege}}<li>Puissance piège: {{pui_piege}}</li>{{/if}}
            {{#if renvoie_dmg}}<li>Renvoie de dommage: {{renvoie_dmg}}</li>{{/if}}
            {{#if bou}}<li>Bouclier: {{bou}}</li>{{/if}}
            {{#if dmg_f_neutre}}<li>Dommage fix neutre: {{dmg_f_neutre}}</li>{{/if}}
            {{#if dmg_f_terre}}<li>Dommage fix terre: {{dmg_f_terre}}</li>{{/if}}
