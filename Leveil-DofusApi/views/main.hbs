            {{#if ret_pa}}<li>Retrait PA: {{ret_pa}}</li>{{/if}}
            {{#if ret_pm}}<li>Retrait PM: {{ret_pm}}</li>{{/if}}
            {{#if pod}}<li>Pods: {{pod}}</li>{{/if}}
