<!DOCTYPE html>
<html>
    <head>    
        <title>{{title}}</title>
    </head>
    <body class="container">  
        <h1>{{titre}}</h1>  
        <p id="main-paragraphe" class="center">{{content}}</p>
        <ul>
            <li>Nom: {{name}}</li>
            {{#if lvl}}<li>Lvl: {{lvl}}</li>{{/if}}
            {{#if pv}}<li>PV: {{pv}}</li>{{/if}}
            {{#if pa}}<li>PA: {{pa}}</li>{{/if}}
            {{#if pm}}<li>PM: {{pm}}</li>{{/if}}
            {{#if po}}<li>PO: {{po}}</li>{{/if}}
            {{#if ini}}<li>Initiative: {{ini}}</li>{{/if}}
            {{#if invo}}<li>Invocation: {{invo}}</li>{{/if}}
            {{#if critique}}<li>Critique: {{critique}}</li>{{/if}}
            {{#if prosp}}<li>Propsection: {{prosp}}</li>{{/if}}
            {{#if soin}}<li>Soins: {{soin}}</li>{{/if}}
            {{#if sag}}<li>Sagesse: {{sag}}</li>{{/if}}
            {{#if fo}}<li>Force: {{fo}}</li>{{/if}}
            {{#if int}}<li>Inteligence: {{int}}</li>{{/if}}
            {{#if cha}}<li>Chance: {{cha}}</li>{{/if}}
            {{#if agi}}<li>Agilité: {{agi}}</li>{{/if}}
            {{#if pui}}<li>Puissance: {{pui}}</li>{{/if}}
            {{#if fui}}<li>Fuite: {{fui}}</li>{{/if}}
